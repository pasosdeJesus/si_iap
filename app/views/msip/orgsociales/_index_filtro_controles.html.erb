<%= f.input :bustipoorg_id, 
  as: :hidden,
  input_html: {
    value: params && params[:filtro] && params[:filtro][:bustipoorg_id] ? 
      params[:filtro][:bustipoorg_id] : nil
  } 
%>
<%= f.button :submit, 
  name: 'filtrar',
